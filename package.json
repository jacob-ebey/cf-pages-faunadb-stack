{
  "private": true,
  "name": "remix-app-template",
  "description": "",
  "license": "",
  "dependencies": {
    "@reach/alert": "^0.16.0",
    "@remix-run/cloudflare-pages": "^1.2.1",
    "@remix-run/react": "^1.2.1",
    "faunadb": "^4.5.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remix": "^1.2.1",
    "tiny-invariant": "^1.2.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.2.0",
    "@fauna-labs/fauna-schema-migrate": "^2.2.1",
    "@remix-run/dev": "^1.2.1",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "patch-package": "^6.4.7",
    "typescript": "^4.1.2",
    "wrangler": "beta"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false,
  "scripts": {
    "postinstall": "patch-package && remix setup cloudflare-pages",
    "build": "cross-env NODE_ENV=production remix build",
    "dev:remix": "cross-env NODE_ENV=development remix watch",
    "dev:wrangler": "source .env && wrangler pages dev ./public -b FAUNA_DOMAIN=${FAUNA_DOMAIN} -b FAUNA_PORT=${FAUNA_PORT} -b FAUNA_SCHEME=${FAUNA_SCHEME} -b FAUNA_SECRET=${FAUNA_SECRET} -b SESSION_SECRET=${SESSION_SECRET}",
    "dev": "cross-env NODE_ENV=development remix build && run-p dev:*",
    "start": "cross-env NODE_ENV=production npm run dev:wrangler"
  }
}
